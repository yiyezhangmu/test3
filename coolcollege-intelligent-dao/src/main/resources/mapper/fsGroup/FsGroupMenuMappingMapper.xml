        insert into fs_group_menu_mapping_${eid}(menu_id, chat_id, level_id, create_user, create_time)
        values
        <foreach collection="dos" item="do" separator=",">
        (#{do.menuId}, #{do.chatId}, #{do.levelId}, #{do.createUser}, #{do.createTime})
        </foreach>
    </insert>

    <insert id="insertOrUpdateBatch" keyProperty="dos.id" useGeneratedKeys="true">
        insert IGNORE into fs_group_menu_mapping_${eid}(menu_id, chat_id, level_id, create_user, create_time)
        values
        <foreach collection="dos" item="do" separator=",">
            (#{do.menuId}, #{do.chatId}, #{do.levelId}, #{do.createUser}, #{do.createTime})
        </foreach>
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update fs_group_menu_mapping_${eid}
        <set>
            <if test="fsGroupMenuMappingDO.menuId != null">
                menu_id = #{fsGroupMenuMappingDO.menuId},
            </if>
            <if test="fsGroupMenuMappingDO.chatId != null and fsGroupMenuMappingDO.chatId != ''">
                chat_id = #{fsGroupMenuMappingDO.chatId},
