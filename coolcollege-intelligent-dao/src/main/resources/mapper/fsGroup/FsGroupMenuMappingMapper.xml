        where chat_id = #{chatId}
    </select>
    <select id="queryIdAndNameByChatId" resultType="com.coolcollege.intelligent.model.fsGroup.vo.IdAndNameVO">
        select
            f.id as id,f.menu_name as name
        from fs_group_menu_mapping_${eid} fm
                 inner join fs_group_menu_${eid} f
                            on f.id=fm.menu_id
        where fm.chat_id = #{chatId}
    </select>
    <select id="queryByMenuId" resultType="com.coolcollege.intelligent.model.fsGroup.FsGroupMenuMappingDO">
        select
        <include refid="baseSql"></include>
        from fs_group_menu_mapping_${eid}
        where menu_id = #{menuId}
    </select>
    <select id="queryMenuByChatId" resultType="java.lang.Long">
