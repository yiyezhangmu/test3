    </delete>
    <delete id="deleteByChatId">
        delete from fs_group_menu_mapping_${eid} where chat_id = #{chatId}
    </delete>
    <delete id="deleteByMenuId">
        delete from fs_group_menu_mapping_${eid} where menu_id = #{menuId}
    </delete>

</mapper>

