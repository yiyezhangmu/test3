        select
          <include refid="baseSql"></include>
        from fs_group_menu_mapping_${eid}
        where id = #{id}
    </select>

    <!--统计总行数-->
    <select id="count" resultType="java.lang.Long">
        select count(1)
        from fs_group_menu_mapping_${eid}
        <where>
            <if test="fsGroupMenuMappingDO.id != null">
                and id = #{fsGroupMenuMappingDO.id}
            </if>
            <if test="fsGroupMenuMappingDO.menuId != null">
                and menu_id = #{fsGroupMenuMappingDO.menuId}
            </if>
            <if test="fsGroupMenuMappingDO.chatId != null and fsGroupMenuMappingDO.chatId != ''">
