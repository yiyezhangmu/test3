<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.coolcollege.intelligent.dao.user.UserJurisdictionStoreMapper">
    <resultMap id="BaseResultMap" type="com.coolcollege.intelligent.model.user.UserJurisdictionStoreDO">
        <id property="id" column="id" jdbcType="BIGINT"/>
        <result property="userId" column="user_id" jdbcType="VARCHAR"/>
        <result property="storeId" column="store_id" jdbcType="VARCHAR"/>
        <result property="createTime" column="create_time" jdbcType="TIMESTAMP"/>
    </resultMap>
    <insert id="batchInsertUserJurisdictionStore" parameterType="com.coolcollege.intelligent.model.user.UserJurisdictionStoreDO" >
        insert into user_jurisdiction_store_${eid}
        (
         user_id,
         store_id
         )
        values
        <foreach collection="items" item="item" separator=",">
            (
             #{item.userId},
             #{item.storeId}
             )
        </foreach>
    </insert>

    <delete id="deleteUserJurisdictionStoreByUserId">
        delete from user_jurisdiction_store_${enterpriseId} where user_id = #{userId}
    </delete>

</mapper>