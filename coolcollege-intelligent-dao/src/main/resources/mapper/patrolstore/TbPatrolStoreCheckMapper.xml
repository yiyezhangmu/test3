            region_way as 'regionWay',
            store_id as 'storeId',
            supervisor_name AS 'supervisorName',
            supervisor_id AS 'supervisorId',
            supervisor_job_num AS 'supervisorJobNum',
            COUNT(supervisor_id) AS 'checkTotalNum',
            SUM(CASE WHEN big_region_check_status = 1 THEN 1 ELSE 0 END) AS 'bigRegionCheckNum',
            SUM(CASE WHEN big_region_check_status = 0 THEN 1 ELSE 0 END) AS 'bigRegionNotCheckNum',
            SUM(CASE WHEN war_zone_check_status = 1 THEN 1 ELSE 0 END) AS 'warCheckNum',
            SUM(CASE WHEN war_zone_check_status = 0 THEN 1 ELSE 0 END) AS 'warNotCheckNum'
    FROM tb_patrol_store_check_${enterpriseId}

    <include refid="patrolStoreCheckQuery"/>
    GROUP BY supervisor_id
    </select>

  <select id="selectListBySupervisorId" resultMap="BaseResultMap">
    select supervisor_id , meta_table_ids
    from tb_patrol_store_check_${enterpriseId}
    where supervisor_id in
      <foreach collection="supervisorIds" item="supervisorId" open="(" separator="," close=")">
              #{supervisorId}
      </foreach>
    and deleted = 0
  </select>

  <select id="getPatrolStoreCheckListById" resultType="com.coolcollege.intelligent.model.patrolstore.statistics.PatrolStoreCheckVO">
    select tcd.id FROM tb_data_sta_table_column_${enterpriseId} tcd
    JOIN tb_patrol_store_check_${enterpriseId} tps
    ON tcd.business_id = tps.business_id
    <where>
      <if test="query.storeIdList != null and query.storeIdList.size() > 0">
        and tps.store_id in (
        <foreach collection="query.storeIdList" item="storeId" separator=",">
          #{storeId}
        </foreach>
        )
      </if>
      <if test="query.regionPathList != null and query.regionPathList.size() > 0">
        <foreach collection="query.regionPathList" separator="or" close=")" open="and (" item="region">
          tps.region_way like concat(#{region}, '%')
        </foreach>
      </if>
      <if test="query.regionPath != null and query.regionPath != ''">
        and tps.region_way like concat(#{query.regionPath},'%')
      </if>
      <if test="query.beginDate != null and query.endDate != null">
        and tps.sign_end_time between #{query.beginDate} and #{query.endDate}
      </if>
      <if test="query.taskBeginDate != null and query.taskEndDate != null">
        and tps.sub_end_time between #{query.taskBeginDate} and #{query.taskEndDate}
      </if>
      <if test="query.metaTableId != null">
        and tcd.meta_table_id =#{query.metaTableId}
      </if>
      <if test="query.patrolType != null and query.patrolType != ''">
        and tps.patrol_type = #{query.patrolType}
      </if>
      <if test="query.supervisorId != null and query.supervisorId.size() > 0">
        and tps.supervisor_id in
        <foreach collection="query.supervisorId" item="supervisorId" separator="," open="(" close=")">
          #{supervisorId}
        </foreach>
      </if>
      <if test="query.supervisorJobNum != null and query.supervisorJobNum != ''">
        and tps.supervisor_job_num =#{query.supervisorJobNum}
      </if>
      <if test="query.taskName != null and query.taskName != ''">
        and tps.task_name like concat('%', #{query.taskName}, '%')
      </if>
      <if test="query.storeNum != null and query.storeNum != ''">
        and tps.store_num like concat('%', #{query.storeNum}, '%')
      </if>
      <if test="query.bigRegionCheckStatus != null ">
        and tps.big_region_check_status = #{query.bigRegionCheckStatus}
      </if>
      <if test="query.warZoneCheckStatus != null ">
        and tps.war_zone_check_status = #{query.warZoneCheckStatus}
      </if>
      <if test="query.bigRegionUserId != null and query.bigRegionUserId.size() > 0">
        and tps.big_region_user_id in
        <foreach collection="query.bigRegionUserId" item="bigRegionUserId" separator="," open="(" close=")">
          #{bigRegionUserId}
        </foreach>
      </if>
      <if test="query.bigRegionUserJobNum != null and query.bigRegionUserJobNum != ''">
        and tps.big_region_user_job_num like concat('%',#{query.bigRegionUserJobNum},'%')
      </if>
      <if test="query.warZoneUserId != null and query.warZoneUserId.size() > 0">
        and tps.war_zone_user_id in
        <foreach collection="query.warZoneUserId" item="warZoneUserId" separator="," open="(" close=")">
          #{warZoneUserId}
        </foreach>
      </if>
      <if test="query.warZoneUserJobNum != null and query.warZoneUserJobNum != ''">
        and tps.war_zone_user_job_num like concat('%',#{query.warZoneUserJobNum},'%')
      </if>
      <if test="query.id != null ">
        and tps.id = #{query.id}
      </if>
      and tps.deleted = 0
    </where>
  </select>

  <select id="getPatrolStoreCheckListByBusinessId" resultMap="BaseResultMap">
    select *
    from tb_patrol_store_check_${enterpriseId}
    where business_id in
      <foreach collection="businessIds" item="businessId" open="(" separator="," close=")">
              #{businessId}
      </foreach>
    and deleted = 0
  </select>
</mapper>
